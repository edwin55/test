  <node componentName="tJava" componentVersion="0.101" offsetLabelX="0" offsetLabelY="0" posX="512" posY="64">
    <elementParameter field="TEXT" name="UNIQUE_NAME" value="tJava_1" show="false"/>
    <elementParameter field="MEMO_JAVA" name="CODE" value="try {&#xA;    //var globales&#xA;    //String context.url=&quot;https://www.cia.gov/library/publications/world-leaders-1/&quot;;&#xA;    //String context.destino=&quot;D:/ListasNegras/CIA/&quot;;&#xA;    //String context.extension=&quot;.txt&quot;;&#xA;    String urlPais=&quot;&quot;; //guarda la urls para consultar autoridades&#xA;    String nombrePais=&quot;&quot;; //guarda los paises para crear archivos&#xA;    //Parte 1. Obtiene listado de paises&#xA;    Document home = Jsoup.connect(context.url).get(); //consulta url&#xA;    Element homeContent = home.getElementById(&quot;cosCountryList&quot;); //nodo listado de paises&#xA;    Elements links = homeContent.getElementsByTag(&quot;a&quot;); //paises&#xA;    for (Element link : links) {&#xA;        //String linkHref = link.attr(&quot;href&quot;); //url del pais&#xA;        //String linkText = link.text(); //nombre del pais&#xA;        String[] compuestoPais=link.text().split(&quot;,&quot;); //nombre separado x coma&#xA;        if(compuestoPais.length>1){ //nombre completo sin comas&#xA;            nombrePais+=compuestoPais[1].replaceFirst(&quot;^\\s&quot;, &quot;&quot;)+&quot; &quot;+compuestoPais[0]+&quot;;&quot;; //nombre&#xA;        }else{&#xA;            nombrePais+=compuestoPais[0]+&quot;;&quot;; //nombre corto&#xA;        }&#xA;        urlPais+=link.attr(&quot;href&quot;)+&quot;;&quot;; //url del pais&#xA;        //System.out.println(linkHref);&#xA;        //System.out.println(linkText);&#xA;    } //end for&#xA;    //Parte 2. Obtiene autoridades del pais&#xA;    String[] archivo=nombrePais.split(&quot;;&quot;); int j=0;&#xA;    for(String uri : urlPais.split(&quot;;&quot;)){ //consulta urls&#xA;        Document page = Jsoup.connect(context.url+uri).get(); //consulta pagina de autoridades x pais&#xA;        Elements cargos = page.getElementsByClass(&quot;title&quot;); //nodo de cargos&#xA;        Elements autoridades = page.getElementsByClass(&quot;cos_name&quot;); //nodo de nombre de autoridades&#xA;        int i=0; //crear archivo con nombre del pais&#xA;        //System.out.println(cargos.get(0).getElementsByTag(&quot;span&quot;).last().text());&#xA;        //System.out.println(autoridades.get(0).getElementsByTag(&quot;span&quot;).last().text());       &#xA;        try (Writer guarda = new BufferedWriter(new OutputStreamWriter( new FileOutputStream(context.destino+archivo[j++]+context.extension), &quot;utf-8&quot;))) {&#xA;            for(Element cargo : cargos){ //recorre todos los cargo del pais&#xA;                Element autoridad = autoridades.get(i++); //obtiene el nombre de autoridad de cada cargo&#xA;                guarda.write(cargo.getElementsByTag(&quot;span&quot;).last().text()); //guarda cargo&#xA;                guarda.write(&quot;\t&quot;);&#xA;                guarda.write(autoridad.getElementsByTag(&quot;span&quot;).last().text()); //guarda nombre de la autoridad&#xA;                guarda.write(&quot;\r\n&quot;);&#xA;                //System.out.println(cargo.getElementsByTag(&quot;span&quot;).last().text());&#xA;                //System.out.println(autoridad.getElementsByTag(&quot;span&quot;).last().text());&#xA;                //System.out.println(&quot;&quot;);&#xA;            } //end for&#xA;            guarda.close(); //cierra archivo de pais            &#xA;            //System.out.println(contentPage.get(2).getElementsByTag(&quot;span&quot;).last().text());&#xA;        } //end writter       &#xA;    } // end for&#xA;    &#xA;} catch (Exception ex) {&#xA;    //Logger.getLogger(JavaApp.class.getName()).log(Level.SEVERE, null, ex);&#xA;    System.out.println(&quot;Error: Descarga de lista de jefes de estado&quot;);&#xA;}"/>
    <elementParameter field="MEMO_IMPORT" name="IMPORT" value="//import java.util.List;&#xA;import org.jsoup.Jsoup;&#xA;import org.jsoup.nodes.Document;&#xA;import org.jsoup.nodes.Element;&#xA;import org.jsoup.select.Elements;&#xA;import java.io.FileOutputStream;&#xA;import java.io.OutputStreamWriter;&#xA;import java.io.BufferedWriter;&#xA;import java.io.IOException;&#xA;import java.io.Writer;"/>
    <elementParameter field="TEXT" name="CONNECTION_FORMAT" value="row"/>
