  <dependencies>
    <dependency>
      <groupId>org.talend.libraries</groupId>
      <artifactId>commons-codec-1.6</artifactId>
      <version>6.0.0</version>
      <scope>system</scope>
      <systemPath>${system.lib.path}/commons-codec-1.6.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>org.talend.libraries</groupId>
      <artifactId>commons-httpclient-3.0.1</artifactId>
      <version>6.0.0</version>
      <scope>system</scope>
      <systemPath>${system.lib.path}/commons-httpclient-3.0.1.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>org.talend.libraries</groupId>
      <artifactId>jcifs-1.3.0</artifactId>
      <version>6.0.0</version>
      <scope>system</scope>
      <systemPath>${system.lib.path}/jcifs-1.3.0.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>org.talend.libraries</groupId>
      <artifactId>dom4j-1.6.1</artifactId>
      <version>6.0.0</version>
      <scope>system</scope>
      <systemPath>${system.lib.path}/dom4j-1.6.1.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>org.talend.libraries</groupId>
      <artifactId>log4j-1.2.16</artifactId>
      <version>6.0.0</version>
      <scope>system</scope>
      <systemPath>${system.lib.path}/log4j-1.2.16.jar</systemPath>
