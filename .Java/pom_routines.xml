<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.talend.master</groupId>
    <artifactId>master.Codes</artifactId>
    <version>6.0.0</version>
    <relativePath>./pom.xml</relativePath>
  </parent>
  <groupId>org.talend.code</groupId>
  <artifactId>routines</artifactId>
  <version>6.0.0</version>
  <properties>
    <talend.routines.package>routines</talend.routines.package>
  </properties>
  <dependencies>
    <dependency>
      <groupId>org.talend.libraries</groupId>
      <artifactId>log4j-1.2.16</artifactId>
      <version>6.0.0</version>
      <scope>system</scope>
      <systemPath>${system.lib.path}/log4j-1.2.16.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>org.talend.libraries</groupId>
      <artifactId>dom4j-1.6.1</artifactId>
      <version>6.0.0</version>
      <scope>system</scope>
      <systemPath>${system.lib.path}/dom4j-1.6.1.jar</systemPath>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-clean-plugin</artifactId>
        <executions>
          <execution>
            <id>default-clean</id>
            <phase>none</phase>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <configuration>
          <includes>
            <include>${talend.routines.package}/**/*.class</include>
          </includes>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>